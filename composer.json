{
    "name": "setono/sylius-terms-plugin",
    "type": "sylius-plugin",
    "description": "Sylius terms and conditions plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "terms"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "doctrine/collections": "^1.6",
        "doctrine/orm": "^2.7",
        "knplabs/knp-menu": "^3.1",
        "sylius/resource-bundle": "^1.6",
        "symfony/config": "^5.4 || ^6.4 || ^7.0",
        "symfony/dependency-injection": "^5.4 || ^6.4 || ^7.0",
        "symfony/form": "^5.4 || ^6.4 || ^7.0",
        "symfony/http-foundation": "^4.4 || ^5.0.0",
        "symfony/http-kernel": "^4.4 || ^5.1.5",
        "symfony/options-resolver": "^5.4 || ^6.4 || ^7.0",
        "symfony/routing": "^5.4 || ^6.4 || ^7.0",
        "symfony/translation-contracts": "^1.1 || ^2.4 || ^3.4",
        "twig/twig": "^2.14 || ^3.8",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-phpunit": "^0.18.4",
        "setono/code-quality-pack": "^2.6",
        "sylius/sylius": "~1.12.13",
        "symfony/debug-bundle": "^5.4 || ^6.4 || ^7.0",
        "symfony/dotenv": "^5.4 || ^6.4 || ^7.0",
        "symfony/intl": "^5.4 || ^6.4 || ^7.0",
        "symfony/web-profiler-bundle": "^5.4 || ^6.4 || ^7.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true,
            "symfony/thanks": false
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\SyliusTermsPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\SyliusTermsPlugin\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
